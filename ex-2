// creating input and output for integer using java  

import java.util.Scanner;
public class Main{
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    int x = input.nextInt();
    System.out.print(" Your entered number is : " + x);
    }
}


/* another way to print */ 

import java.util.Scanner;
class Main{
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    System.out.print("Your entered number is : " + input.nextInt());
  }
}
